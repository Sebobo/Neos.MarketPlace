prototype(Neos.MarketPlace:VersionMenu) >
prototype(Neos.MarketPlace:VersionMenu) < prototype(TYPO3.TypoScript:Tag) {
    tagName = 'ul'
    attributes {
        class = 'nav nav--vertical'
    }
    content = TYPO3.TypoScript:Collection {
        collection = ${q(documentNode).find('versions').find('[instanceof Neos.MarketPlace:Version]')}
        itemRenderer = Neos.MarketPlace:VersionMenuItem
        itemName = 'node'
    }
}
