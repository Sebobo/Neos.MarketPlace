prototype(Neos.MarketPlace:Vendor.Document) < prototype(Neos.MarketPlace:Document) {
    body {
        templatePath = 'resource://Neos.MarketPlace/Private/Templates/Documents/Vendor.html'

        packages = Neos.MarketPlace:PackageMenu

        search = Neos.MarketPlace:Search {
            actionNode = Neos.MarketPlace:ClosestRepositoryStorageNode
        }

        content {
            vendorName = ${q(documentNode).property('title')}
        }
    }
}
