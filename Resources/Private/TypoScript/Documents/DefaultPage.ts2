prototype(Page) {
    head {
        stylesheets.marketPlace = TypoScript:Tag {
            tagName = 'link'
            attributes {
                rel = 'stylesheet'
                href = TypoScript:ResourceUri {
                    path = 'resource://Neos.MarketPlace/Public/StyleSheets/Application.css'
                }
            }
            @position = 'after site'
        }
    }
}
